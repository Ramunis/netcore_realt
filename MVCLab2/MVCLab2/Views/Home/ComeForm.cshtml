@{
    ViewData["Title"] = "Личный кабинет";

}
@model IEnumerable<MVCLab2.Models.myboard>

<div class="text-center">

    <h1>Личный кабинет</h1>
    <table class="table table-striped">
        <thead>
            <tr>
                <th scope="col">ДЗ</th>
                <th scope="col">Клиент</th>
                <th scope="col">Реалтор</th>
                <th scope="col">Район</th>
                <th scope="col">Услуга</th>
                <th scope="col">Площадь</th>
                <th scope="col">ДС</th>
                <th scope="col">Срок</th>
                <th scope="col">Цена</th>
                <th scope="col">Оплата</th>
                <th scope="col">Ремонт</th>
                <th scope="col"></th>
            </tr>
        </thead>
        <tbody>

            @foreach (var item in Model)
            {
            <tr>
                <td>@item.DZ</td>
                <td>@item.Clientf</td>
                <td>@item.Realtf</td>
                <td>@item.District</td>
                <td>@item.Servic</td>
                <td>@item.Sq</td>
                <td>@item.DS</td>
                <td>@item.Term</td>
                <td>@item.Price</td>
                <td>@item.Pay</td>
                <td>@item.Repair</td>
                <td><a asp-controller="Home" asp-action="Deal" asp-route-id=@item.ID>Подробней</a></td>
            </tr>
            }

        </tbody>
    </table>
    <div class="come">
        @Html.ActionLink("Назад", "ReadDB")
    </div>
</div>